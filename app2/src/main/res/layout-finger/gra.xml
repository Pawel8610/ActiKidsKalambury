<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1"
    android:background="#3fd2ed">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView9"
        android:maxHeight="42dp"
        android:adjustViewBounds="true"
        android:src="@drawable/help"
        android:scaleType="fitCenter"
        android:layout_marginTop="5dp"
        android:layout_centerHorizontal="true"
         />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitCenter"
        android:src="@drawable/z0"
        android:id="@+id/imageView3"
        android:maxHeight="100dp"
        android:layout_marginTop="55dp"
        android:adjustViewBounds="true"
        android:layout_centerHorizontal="true" />
    <ImageView
        android:id="@+id/imageViewProtect"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitCenter"
        android:layout_below="@+id/imageView3"
        android:layout_above="@+id/imageView5"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"

        />
    <!--android:src="@drawable/p1" path do zdjÄ™cia p1.png-->
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:scaleType="fitCenter"
        android:layout_above="@+id/imageView5"
        android:layout_below="@+id/imageView3"
        android:layout_alignTop="@+id/imageViewProtect"
        android:layout_alignLeft="@+id/imageViewProtect"
        android:layout_centerInParent="true"
         />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitCenter"
        android:id="@+id/imageViewFrame"
        android:layout_below="@+id/imageView3"
        android:layout_above="@+id/imageView5"
        android:layout_alignTop="@+id/imageViewProtect"
        android:layout_alignLeft="@+id/imageViewProtect"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitCenter"
        android:id="@+id/imageViewprzejsciowe"
        android:layout_below="@+id/imageView3"
        android:layout_above="@+id/imageView5"
        android:layout_alignTop="@+id/imageViewProtect"
        android:layout_alignLeft="@+id/imageViewProtect"
         />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/banner"
        android:scaleType="fitCenter"
        android:layout_alignParentBottom="true"
        android:adjustViewBounds="true"
        android:maxHeight="60dp"
        android:layout_marginBottom="0dp"
        android:alpha="0" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:maxHeight="64dp"
        android:adjustViewBounds="true"
        android:onClick="SaveSettings"
        android:layout_above="@+id/imageView2"
        android:layout_marginBottom="5dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:scaleType="fitCenter"
        android:src="@drawable/save" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView6"
        android:onClick="Losuj"
        android:maxHeight="55dp"
        android:adjustViewBounds="true"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/imageViewProtect"
        android:layout_above="@+id/imageView2"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="15dp"
        android:scaleType="fitCenter"
        android:src="@drawable/play" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView7"
        android:maxHeight="50dp"
        android:src="@drawable/timer"
        android:adjustViewBounds="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="0dp"
        android:scaleType="fitCenter"
        android:layout_alignParentBottom="false"
        android:layout_alignWithParentIfMissing="false"
        android:cropToPadding="false"
        android:focusable="false"
        android:focusableInTouchMode="false" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/textView5"
        android:textColor="#ff0000"
        android:textSize="20dp"
        android:layout_marginLeft="46dp"
        android:layout_marginTop="16dp"
        android:text="30s" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView8"
        android:maxHeight="50dp"
        android:src="@drawable/licznik"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:layout_marginTop="5dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="00/00"
        android:id="@+id/textView4licznik"
        android:layout_marginRight="42dp"
        android:layout_marginTop="16dp"
        android:layout_alignRight="@+id/imageView5"
        android:layout_alignEnd="@+id/imageView5"
        android:textSize="20dp" />
    <com.google.android.gms.ads.AdView
        android:id="@+id/ad_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        ads:adSize="BANNER"
        ads:adUnitId="@string/banner_ad_unit_id_game" />



</RelativeLayout>